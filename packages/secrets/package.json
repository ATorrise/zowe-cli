{
  "name": "@zowe/secrets-for-zowe-sdk",
  "description": "Credential management facilities for Imperative, Zowe CLI, and extenders.",
  "repository": "https://github.com/zowe/zowe-cli.git",
  "author": "Zowe",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "prebuilds",
    "src",
    "index.d.ts",
    "index.js"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.2",
    "ava": "^4.3.3"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "cd src/keyring && napi build --config napi.json --platform --release",
    "build:debug": "cd src/keyring && napi build --config napi.json --platform",
    "install": "node scripts/platformCheck.js || npm run rebuild",
    "prepublishOnly": "bash scripts/prebuildify.sh",
    "rebuild": "npx --yes --package=@napi-rs/cli@2.16.2 -- napi build --config src/keyring/napi.json --cargo-cwd src/keyring --platform --release --no-dts-header --js=false prebuilds",
    "test": "ava",
    "version": "napi version"
  }
}
